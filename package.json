{
  "name": "@wke-baas/postgrest-client",
  "version": "0.2.4",
  "description": "A simple postgrest client integrated with standard schema for validating responses.",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "bunup": "^0.16.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "valibot": "^1.2.0"
  }
}
